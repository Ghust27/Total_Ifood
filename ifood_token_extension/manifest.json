{
  "manifest_version": 3,
  "name": "iFood Token Finder",
  "version": "1.0",
  "permissions": [
    "webRequest",
    "storage",
    "activeTab",
    "scripting",
    "clipboardWrite"
  ],
  "host_permissions": [
    "<all_urls>"
  ],
  "background": {
    "service_worker": "background.js"
  },
  "action": {
    "default_popup": "popup.html"
  }
}
